/* CSS Document */
/*公共样式类 开始*/
/*强制不换行处理 末尾加点点点*/
.overflow-ellipsis {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.Setting {
  position: absolute;
  left: 0px;
  top: 0px;
  width: 2000px;
  height: 1160px;
  z-index: 54;
}
.body-style {
  background-color: @BackBg;
  font-size: 14px;
  color: @ConventionalText;
}
/*操作按钮*/
.action-button {
  width: 58px;
  height: 28px;
  display: inline-block;
  border: 1px solid #cccccc;
  border-radius: 3px;
  text-align: center;
  font-size: 14px;
  line-height: 28px;
}
/*公共样式类 结束*/
/*数据源列表展示页面 开始*/
.data-list .daohang .type-list {
  font-size: 14px;
  color: #333333;
  padding-top: 10px;
}
.data-list .daohang .type-list > li {
  height: 30px;
  line-height: 30px;
  padding-left: 28px;
  padding-right: 24px;
  margin-top: 10px;
  margin-bottom: 10px;
}
.data-list .daohang .type-list > li.active {
  background-color: #f6f6f6;
  color: #0066ff;
}
.data-list .daohang .type-list > li.active > a {
  color: #0066ff;
}
.data-list .daohang .type-list > li > a {
  float: left;
  width: 10em;
}
.data-list .daohang .type-list > li > span {
  width: 3em;
  overflow: hidden;
}
.data-list .daohang .type-list > li > span {
  float: right;
}
.data-list .daohang .type-list > li:hover {
  background-color: #f6f6f6;
  color: #0066ff;
}
.data-list .daohang .type-list > li:hover > a {
  color: #0066ff;
}
.data-list .nav_2 {
  background-color: @PanelBg;
  background-image:@TipTitleBg;
}
.data-list .state-list {
  height: 98px;
  line-height: 98px;
  background-image: @TipTitleBg;
}
.data-list .state-list .nav_2_div {
  width: auto;
}
.data-list .state-list > li {
  float: left;
  width: 240px;
  height: 98px;
  line-height: 98px;
  text-align: center;
  cursor: pointer;
}
.data-list .state-list > li:hover {
  float: left;
  width: 240px;
  height: 98px;
  line-height: 98px;
  text-align: center;
  cursor: pointer;
}
.data-list .state-list > li .show {
  display: inline-block;
  width: 102px;
  height: 90px;
  text-align: center;
  line-height: 90px;
}
.data-list .state-list > li .show + .line {
  display: inline-block;
  width: 1px;
  height: 64px;
  background-color: #ddd;
  vertical-align: middle;
  float: right;
  margin-top: 18px;
}
.data-list .state-list > li.active .show {
  border-bottom: 2px solid #2968f6;
}
.data-list .state-list > li:hover .show {
  border-bottom: 2px solid #2968f6;
}
.data-list .state-list > li .show > ul {
  display: inline-block;
  vertical-align: middle;
}
.data-list .state-list > li .show > ul > li {
  line-height: 40px;
}
.data-list .state-list > li .show > ul > li.sz {
  font-size: 30px;
  color: #0066ff;
  display: block;
}
.data-list .state-list > li .show > ul > li.type {
  font-size: 14px;
  color: #999999;
  display: block;
  width: 5em;
}
.state-list .nav_2_div_a i {
  display: block;
  width: 25px;
  height: 25px;
  margin: 30px auto 6px;
  background: url(../img/add-icon.png) no-repeat;
}
.list-table {
  max-width: 1550px;
  min-width: 932px;
  width: 98%;
  border: 1px solid #eee;
  margin: 20px auto;
  background-color: #fff;
  border: 1px solid #dde2ec;
  font-size: 16px;
  color: #333333;
}
.list-table > thead {
  height: 46px;
  background-color: #dddddd;
  color: #333333;
}
.list-table > thead > tr {
  height: 46px;
  box-sizing: border-box;
  border: 1px solid #ddd;
}
.list-table > thead > tr > th,
.list-table > tbody > tr > td {
  text-align: left;
  padding-left: 30px;
  font-weight: normal;
}
.list-table > thead > tr > th + th {
  border-left: 1px solid #fff;
}
.list-table > tbody > tr {
  height: 80px;
  border-bottom: 1px solid #dde2ec;
}
.list-table > tbody > tr:nth-of-type(even) {
  background-color: #f6f6f6;
}
.list-table > tbody > tr > td + td {
  border-left: 1px solid #dde2ec;
}
.list-table > thead > tr > .th1 {
  min-width: 250px;
}
.list-table > thead > tr > .th2 {
  width: 200px;
}
.list-table > thead > tr > .th3 {
  width: 200px;
}
.list-table > thead > tr > .th4 {
  width: 200px;
}
.list-table > tbody > tr > .name > img {
  max-width: 60px;
  display: inline-block;
  vertical-align: middle;
}
.list-table > tbody > tr > .name > span {
  margin-left: 20px;
  margin-right: 10px;
}
/*.list-table>tbody>tr>.action-box{*/
/*padding-left: 0;*/
/*text-align: center;*/
/*}*/
.list-table > tbody > tr > .action-box > .action-button + .action-button {
  margin-left: 10px;
}
/*添加数据源弹窗 开始*/
#add-data-layout {
  display: none;
}
.add-layout-width {
  width: 700px;
  z-index: 100;
}
.icon-box {
  padding: 40px 40px 0 40px;
  font-size: 14px;
  color: #333333;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.data-icon {
  /*width: 140px;*/
  /*height: 198px;*/
  background-color: #ffffff;
  /*border: 1px solid rgb(240, 240, 240);*/
  text-align: center;
  display: inline-block;
  overflow: hidden;
  margin-bottom: 20px;
  margin-right: 20px;
  position: relative;
}
.data-icon:nth-of-type(4n) {
  margin-right: 0;
}
.hover-add {
  background: rgba(255, 255, 255, 0.941);
  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1), 0 16px 24px 0 rgba(0, 0, 0, 0.1);
  width: 100%;
  height: 100%;
  display: none;
  position: absolute;
  top: 0;
  left: 0;
  background-image: url("../img/add-logo.png");
  background-repeat: no-repeat;
  background-position: center;
}
.hover-wait {
  background: rgba(255, 255, 255, 0.941);
  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1), 0 16px 24px 0 rgba(0, 0, 0, 0.1);
  width: 100%;
  height: 100%;
  display: none;
  position: absolute;
  top: 0;
  left: 0;
}
.data-icon:hover > .hover-add {
  display: block;
}
.data-icon:hover > .hover-wait {
  display: block;
}
.hover-wait > span {
  width: 5em;
  display: inline-block;
  margin-top: 50px;
  text-align: left;
  font-size: 14px;
  color: #999999;
}
.data-icon:hover {
  /*background: rgba(255,255,255,.941);*/
  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1), 0 16px 24px 0 rgba(0, 0, 0, 0.1);
}
/*.data-icon:hover:after{*/
/*content: "";*/
/*background-image: url("../img/add-logo.png");*/
/*position: absolute;*/
/*top:0;*/
/*width: 22px;*/
/*height: 24px;*/
/*z-index: 99;*/
/*}*/
.data-icon > img {
  display: block;
  margin: 0 auto;
  width: 140px;
  height: 100px;
}
.data-icon > span {
  display: block;
  margin: 0 auto;
  line-height: 41px;
  width: 140px;
  height: 41px;
  border: 1px solid #f0f0f0;
  box-sizing: border-box;
}
/*添加数据源弹窗 结束*/
/*上传数据 开始*/
.upload-box {
  width: 850px;
  height: 486px;
  margin: 30px auto;
  background-color: @PanelBg;
  border-radius: 5px;
  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1), 0 16px 24px 0 rgba(0, 0, 0, 0.1);
  padding: 60px 77px 0 77px;
  box-sizing: border-box;
  font-size: 16px;
  color:@SecondaryText;
}
.upload-box > .add-file {
  display: block;
  width: 690px;
  height: 240px;
  margin: 0 auto;
  border: 1px dashed #999999;
  box-sizing: border-box;
  border-radius: 5px;
  text-align: center;
  position: relative;
}
.upload-box > .add-file .upload-process {
  display: none;
  vertical-align: middle;
  top: 88px;
  left: 170px;
  position: absolute;
}
.upload-process .file-name {
  text-align: left;
  margin-bottom: 3px;
  display: inline-block;
}
.upload-process .process-bar {
  width: 307px;
  height: 6px;
  background-color: #cccccc;
  border-radius: 20px;
  display: inline-block;
  vertical-align: middle;
  position: relative;
  overflow: hidden;
}
.upload-process .process-bar > span {
  background-color: #0066ff;
  border-bottom-right-radius: 20px;
  display: inline-block;
  position: absolute;
  left: 0;
  border-radius: 20px;
  width: 50%;
  height: 100%;
}
.upload-process .del {
  width: 16px;
  height: 16px;
  background: url("../img/data/del.png") no-repeat center;
  vertical-align: middle;
  display: inline-block;
  cursor: pointer;
}
.upload-process .process-bar + .del {
  margin-left: 20px;
}
.upload-box > .add-file .add-box .add-icon {
  margin: 70px auto 30px;
}
.upload-box > .add-file > .add-box > .add-icon > img,
.upload-box > .add-file > .add-box > .add-icon > .upload {
  display: inline-block;
  vertical-align: middle;
}
.upload-box > .add-file .add-box .add-icon > .upload {
  width: 60px;
  height: 60px;
  background: url("../img/data/upload-add.png") no-repeat center;
  margin: 0 40px;
  position: relative;
  overflow: hidden;
  cursor: pointer;
}
.add-file .add-box .add-icon > img + img {
  margin-left: 40px;
}
.add-file .add-box > .add-icon > .upload > input[type="file"] {
  position: absolute;
  right: -413px;
}
.upload-box > .detail {
  margin-top: 20px;
  font-size: 14px;
}
.upload-box > .demo {
  text-align: center;
  margin-top: 40px;
}
.upload-box > .demo > a {
  display: inline-block;
  box-sizing: border-box;
  width: 100px;
  height: 32px;
  border: 1px solid @AltText;
  border-radius: 3px;
  color: @SecondaryText;
  font-size: 14px;
  text-align: center;
  line-height: 32px;
}
/*上传数据1 结束*/
/*上传数据2 开始*/
.mb-2 .show-data-box {
  width: ~'calc(100% - 200px)';
  min-width: 1200px;
}
.show-data-box {
  width: 1200px;
  /*height: auto;*/
  height: ~'calc(100% - 160px)';
  margin: 30px auto;
  background-color: @PanelBg;
  border-radius: 5px;
  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1), 0 16px 24px 0 rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
  font-size: 16px;
}
.mb-page .show-data-box > .show-file {
  width: ~'calc(100% - 80px)';
  height: ~'calc(100% - 0px)';
  display: block;
  margin: 0 auto;
}
.mb-page .file-content-box .show-data-box > .show-file > .scroll-box {
  /* max-height: 800px; */
  height: ~'calc(100% - 0px)';
}
.mb-2.upload .show-data-box > .show-file {
  width: ~'calc(100% - 70px)';
  height: ~'calc(100% - 30px)';
  display: block;
  margin: 0 auto;
}
.upload .show-data-box > .show-file {
  width: ~'calc(100% - 70px)';
  height: ~'calc(100% - 70px)';
  display: block;
  margin: 0 auto;
}
.show-data-box > .show-file {
  width: ~'calc(100% - 70px)';
  height: ~'calc(100% - 30px)';
  display: block;
  margin: 0 auto;
}
.show-data-box > .show-file > p {
  font-size: 14px;
  color: @SecondaryText;
  display: inline-block;
  padding: 20px 0;
}
.show-data-box > .show-file > p > i {
  color: @navOne;
}
.show-data-box > .show-file > .scroll-box {
  width: 100%;
  height: ~'calc(100% - 60px)';
  padding-top: 0;
  box-sizing: border-box;
  margin: 0 auto;
}
.excel-table {
  width: 1080px;
  border: 1px solid @BackBg;
  background-color: @TableLight;
  font-size: 14px;
  color: @ConventionalText;
}
#tabledata .excel-table {
  width: 100%;
  margin-bottom: 30px;
}
.excel-table .thead {
  font-size: 16px;
  background-color: @TableHeader;
